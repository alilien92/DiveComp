@model IEnumerable<DiveComp.Data.Models.ContestModel>


<div class="col-md-10">
    <table class="table-striped">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Type
                </th>
                <th>
                    Nr. of Jumps
                </th>
                <th>
                    Date
                </th>
                <th>

                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Type.Gender @item.Type.Type @item.Type.Height m
                    </td>
                    <td>
                        @item.Nr_Jumps
                    </td>
                    <td>
                        @item.Date.ToShortDateString()
                    </td>
                    <td>
                        @Html.ActionLink(">>", "SelectedContest", new { id = item.Id })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>