@model IEnumerable<DiveComp.Data.Models.ContestModel>

@{
    ViewData["Title"] = "LoadContest";
}

<h1> Contests</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Club)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Club)
            </td>
            <td>
                @Html.ActionLink("Open", "OpenContest", new {  id=item.Id  })
            </td>
        </tr>
}
    </tbody>
</table>
