@model IEnumerable<DiveComp.Data.Models.EventsModel>

@{
    ViewData["Title"] = "OpenContest";
}

<h1>Events</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Type.Name) @Html.DisplayNameFor(model => model.Type.Height)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Type.Name) @Html.DisplayFor(modelItem => item.Type.Height)
            </td>
            <td>
                @Html.ActionLink("Leaderboard", "Leaderboard", new {  id=item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
