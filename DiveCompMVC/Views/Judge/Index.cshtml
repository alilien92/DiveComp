@model DiveCompMVC.Models.ViewModel


@{
    ViewData["Title"] = "Registered Judges";
}

@Html.ActionLink("Create new judge", "Create", "Judge", null, null)

<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Organization
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.AllJudges)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Org)
                </td>
                <td></td>
            </tr>
        }
    </tbody>
</table>